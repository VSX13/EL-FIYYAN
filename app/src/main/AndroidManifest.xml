<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.el_fiyyan">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/el_fiyyan"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Penjualan"></activity>
        <activity android:name=".ProductActivity" />
        <activity android:name=".Business" />
        <activity android:name=".Developer" />
        <activity android:name=".Profile" />
        <activity android:name=".Email" />
        <activity android:name=".Dashboard" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".product.AddEditProductActivity"
            android:parentActivityName=".ProductActivity"></activity>
    </application>

</manifest>